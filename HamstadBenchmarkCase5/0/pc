FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      pc;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform -7.0291e+07; 

boundaryField
{
    inside
    {
        type                    HAMexternalMoistureFlux;
        value                   uniform -7.0291e+07;        
    }
    outside
    {        
        type                    HAMexternalMoistureFlux;
        value                   uniform -7.0291e+07;              
    }
    sides
    {
        type            empty;
    }
}


