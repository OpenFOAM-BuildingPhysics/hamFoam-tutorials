FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      ws;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -3 0 0 0 0 0];

internalField   uniform 2.0;

boundaryField
{
    inside
    {
        type            zeroGradient;
    }
    outside
    {
        type            zeroGradient;
    }
    sides
    {
        type            empty;
    }
}


