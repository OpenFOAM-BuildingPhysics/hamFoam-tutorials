FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      Ts;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 293.15; 

boundaryField
{

   
    inside
    {     
        type                    HAMexternalHeatFlux;  
        value                   uniform 293.15;  
        Tambient                tableFile; TambientCoeffs {file "0/inside/Tambient";}
        alpha                   tableFile; alphaCoeffs {file "0/inside/alpha";}        
        rad                     tableFile; radCoeffs {file "0/inside/rad";} 
        beta                    tableFile; betaCoeffs {file "0/inside/beta";}
        pv_o                    tableFile; pv_oCoeffs {file "0/inside/pv_o";}
        gl                      tableFile; glCoeffs {file "0/inside/gl";}
        rainTemp                tableFile; rainTempCoeffs {file "0/inside/rainTemp";}               
    }
    outside
    {   
        type                    HAMexternalHeatFlux;  
        value                   uniform 293.15;
        Tambient                tableFile; TambientCoeffs {file "0/outside/Tambient";}
        alpha                   tableFile; alphaCoeffs {file "0/outside/alpha";}        
        rad                     tableFile; radCoeffs {file "0/outside/rad";} 
        beta                    tableFile; betaCoeffs {file "0/outside/beta";}
        pv_o                    tableFile; pv_oCoeffs {file "0/outside/pv_o";}
        gl                      tableFile; glCoeffs {file "0/outside/gl";}
        rainTemp                tableFile; rainTempCoeffs {file "0/outside/rainTemp";}             
    }
    sides
    {
        type            empty;
    }
}


